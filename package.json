{
  "name": "vite-vant-ts",
  "private": true,
  "version": "1.0.0",
  "description": "vant移动端模板",
  "keywords": [
    "vue",
    "vite",
    "vant",
    "typescript"
  ],
  "browserslist": [
    "Android 4.1",
    "iOS 7.1",
    "Chrome > 31",
    "Firefox > 31",
    "ie >= 8",
    "Opera >= 11.5",
    "last 2 versions"
  ],
  "author": "maolihang",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build:deve": "vue-tsc --noEmit && vite build --mode development",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.vue src --fix",
    "prettier": "prettier --write .",
    "tinify": "tinify-images"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "tinify-images": "^1.0.3",
    "ts-md5": "^1.3.1",
    "vant": "^3.5.2",
    "vconsole": "^3.15.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.183",
    "@types/node": "^18.0.6",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-legacy": "^2.0.1",
    "@vitejs/plugin-vue": "^3.0.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
